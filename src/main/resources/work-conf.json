{
  "jobs": [
    {
      "directory": "/home/jsundin/tmp",
      "interval": "1d",
      "retention-policy": "forget",
      "deleted-files-policy": {
        "keep-forever": true
      },
      "old-versions-policy": {
        "keep-forever": true
      }
    }
  ],
  "notifications": [
    {
      "provider": "stdout"
    }
  ],
  "database": {
    "url": "jdbc:derby:s5db;create=true"
  }
}